void setup()
{
  pinMode(13, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(6,OUTPUT);
}

void loop()
{
  digitalWrite(13, HIGH);
  delay(3000);
  digitalWrite(13,LOW);
  delay(3000);
  digitalWrite(9,HIGH);
  delay(3000);
  digitalWrite(9,LOW);
  delay(3000);
  digitalWrite(6,HIGH);
  delay(3000);
  digitalWrite(6,LOW);
  delay(3000);
  
}
